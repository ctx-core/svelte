{
	"name": "@ctx-core/svelte",
	"version": "13.8.0",
	"description": "ctx-core svelte",
	"keywords": [
		"ctx-core",
		"svelte"
	],
	"homepage": "https://github.com/ctx-core/svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./src/index.js",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": {
			"default": "./src/index.js",
			"types": "./src/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^26.12.36",
		"@ctx-core/number": "^8.6.22",
		"@ctx-core/object": "^27.5.0",
		"svelte": "^3.59.1"
	},
	"devDependencies": {
		"c8": "^7.14.0",
		"check-dts": "^0.7.2",
		"locate-character": "^2.0.5",
		"tsx": "^3.12.7",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
