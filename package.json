{
	"name": "@ctx-core/svelte",
	"version": "14.2.15",
	"description": "ctx-core svelte",
	"keywords": [
		"ctx-core",
		"svelte"
	],
	"homepage": "https://github.com/ctx-core/svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./index.js",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./llContext",
		"./store",
		"./_types"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"default": "./index.js",
			"types": "./index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.15",
		"@ctx-core/function": "^21.22.10",
		"@ctx-core/number": "^8.7.15",
		"@ctx-core/object": "^30.4.7",
		"svelte": "^4.2.5"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"locate-character": "^3.0.0",
		"tsx": "^4.1.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}